{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "web",
        "image": "arahbar/web",
        "essential": true,
        "memory": 350,
        "portMappings" : [
            {
                "hostPort": 80,
                "containerPort": 8080
            }
        ],
        "links": [
            "db"
        ]
      },
      {
        "name": "db",
        "image": "arahbar/db",
        "essential": true,
        "memory": 500,
        "environment": [
            {
                "name": "MYSQL_ROOT_PASSWORD",
                "value": "root"
            }
        ]
      },
      {
          "name": "cron",
          "image": "arahbar/cron",
          "essential": true,
          "memory": 100,
          "links": [
              "db"
          ]
      }
    ]
  }